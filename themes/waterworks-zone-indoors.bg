//
// clear to color
//
background
{
    initial_position    0 0
    scroll_speed        0 0
    behavior            DEFAULT
    repeat_x            TRUE
    repeat_y            TRUE

    sprite
    {
        source_file     "images/backgrounds/waterworks-zone-indoors.png"
        source_rect     256 0 256 256
        frame_size      256 256

        animation
        {
            repeat      TRUE
            fps         8
            data        0
        }
    }
}

